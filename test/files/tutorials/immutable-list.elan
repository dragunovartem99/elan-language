# FFFF Elan Beta 2 valid

main
  # A mutable list
  var a set to ["apple", "orange", "pear"]
  set a[0] to "plum"
  call a.add("lemon")
  print a
  # An immutable list...
  var b set to {"apple", "orange", "pear"}
  set b to b.withPutAt(0, "plum") + "lemon"
  print b
  # ...just like manipulating Strings (in any language)
  var c set to "Hello World!"
  set c to c.upperCase()
  print c
end main

# An immutable list (or dictionary) can be created as a constant
constant allFruit set to {"apple", "avocado", "banana", "blueberry", "cherry", "fig", "grape", "kiwi", "lemon", "lychee", "mango", "orange", "papaya", "peach", "pear", "pinapple", "plumb", "raspberry", "strawberry", "watermelon"}
