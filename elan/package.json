{
  "name": "elan",
  "displayName": "elan",
  "description": "elan",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "languages": [
    {
      "id": "elan",
      "extensions": [
        ".elan"
      ],
      "aliases": [
        "Elan",
        "elan"
      ]
    }
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "elan.elanEditor",
        "displayName": "Elan Editor",
        "selector": [
          {
            "filenamePattern": "*.elan"
          }
        ],
        "priority": "default"
      }
    ],
    "taskDefinitions": [
      {
          "type": "elan",
          "when": "resourceExtname == .elan"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "async-file": "2.0.2",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/yauzl": "2.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "jsdom": "^24.0.0",
    "@types/jsdom": "21.1.6"
  }
}
