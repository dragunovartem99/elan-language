{
  "name": "elan",
  "displayName": "elan",
  "description": "elan",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "elan.compile",
        "title": "Elan: Compile"
      }
    ],
    "customEditors": [
      {
        "viewType": "elan.elanEditor",
        "displayName": "Elan Editor",
        "selector": [
          {
            "filenamePattern": "*.elan"
          }
        ],
        "priority": "default"
      }
    ],
    "taskDefinitions": [
      {
          "type": "elan"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "async-file": "2.0.2",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/yauzl": "2.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "runtimeDependencies": [
    {
      "id": "OmniSharp",
      "description": "OmniSharp for Windows (.NET 4.7.2 / x86)",
      "url": "https://roslynomnisharp.blob.core.windows.net/releases/1.39.11/omnisharp-win-x86-1.39.11.zip",
      "installPath": ".omnisharp/1.39.11",
      "platforms": [
        "win32"
      ],
      "architectures": [
        "x86"
      ],
      "installTestPath": "./.omnisharp/1.39.11/OmniSharp.exe",
      "platformId": "win-x86",
      "isFramework": true,
      "integrity": "DF52F6BCEEF14033E8A8C374EF1B81D223FFD17BA9D7E297CFCF0C4BEBF0A22F"
    }
  ]
}
